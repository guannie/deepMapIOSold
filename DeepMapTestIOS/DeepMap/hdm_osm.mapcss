rule building_SelectRule(building) ["isSelected()"] {
    fill-color: #FFF200;
}
 
rule osm.building_SelectRule(osm.building) ["isSelected()"] {
    fill-color: #FFF200;
}
 
rule stand_SelectRule(stand) ["isSelected()"] {
    fill-color: #FFF200;
}
 
rule stand_rooms_SelectRule(stand_rooms) ["isSelected()"] {
