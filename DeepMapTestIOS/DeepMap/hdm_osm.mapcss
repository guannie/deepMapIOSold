feature polygon {
    fill-color: #d0d0d0;
    /*fill-outline-color: #ffffff;*/
    /*fill-outline-width: 0.0;*/
    /*fill-outline-offset: 0.0;*/
    /*fill-translate: [0.1m, 0.1m, 0.1m];*/
    /*fill-pattern: "pattern123.png";*/
    visibility: visible;
    floor-selectable: true;
}

feature line {
    visibility: visible;
    line-color: #f8e9aa;
    line-translate:[0,0,0];
    line-width: 7.0m;
    line-blur: 0.0;
    line-cap: cap-butt;
    line-join: join-round;
    line-gap-width: 0.0m;
    floor-selectable: true;
}
feature icon {
    icon-size: 8.0;
    icon-scaling: icon-scaling-fixed;
    icon-rotation: icon-rotation-rotate;
    icon-rotate: 0;
    floor-selectable: true;
}

/* no-map elements */
feature routing.line:line {
    visibility: visible;
    line-color: #ea857d;
    line-width: 1.0m;
	floor-selectable: true;
}

feature pin.start:polygon {
    fill-color: #81bf97;
}
feature pin.end:polygon {
    fill-color: #ea857d;
}
/* END no-map elements */




